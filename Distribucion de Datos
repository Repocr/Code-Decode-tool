      "cell_type": "code",
      "source": [
        "fig.update_traces(hovertemplate='<b>Región:</b> %{customdata[0]}<br>'+\n",
        "                                  '<b>Año:</b> %{customdata[1]}<br>'+\n",
        "                                  '<b>Trimestre:</b> %{customdata[2]}<br>'+\n",
        "                                  '<b>Ventas:</b> %{y:$.2f}<extra></extra>',\n",
        "                  customdata=df_ventas_trim_reg[['region', 'año', 'trimestre']])\n",
        "\n",
        "fig.show()"
      ],
