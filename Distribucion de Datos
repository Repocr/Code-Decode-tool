      "source": [
        "#### Tratando los datos para generar la visualización"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "df_ventas_trim_reg = ventas.copy()\n",
        "df_ventas_trim_reg = df_ventas_trim_reg[['fecha_pedido','region','ventas']]\n",
        "df_ventas_trim_reg['año'] = df_ventas_trim_reg['fecha_pedido'].dt.year\n",
        "df_ventas_trim_reg['trimestre'] = df_ventas_trim_reg['fecha_pedido'].dt.quarter\n",
        "df_ventas_trim_reg = df_ventas_trim_reg.groupby(['año', 'trimestre', 'region']).agg({'ventas': 'sum'}).reset_index()\n",
        "df_ventas_trim_reg['fecha_trimestre'] = df_ventas_trim_reg['año'].astype(str) + '-Q' + df_ventas_trim_reg['trimestre'].astype(str)\n",
        "df_ventas_trim_reg"
      ],
