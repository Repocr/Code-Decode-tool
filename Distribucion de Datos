    {
      "cell_type": "code",
      "source": [
        "def grafico_linea_ventas_trimestrales(df):\n",
        "    fig, ax = plt.subplots(figsize=(12, 6))\n",
        "    sns.set_theme(style=\"whitegrid\")\n",
        "\n",
        "    # Plotting the main line\n",
        "    sns.lineplot(data=df, x='fecha_pedido', y='ventas', marker='o', color=AZUL2, ax=ax, linewidth=2)\n",
        "\n",
        "    # Highlight Q4 points\n",
        "    q4_data = df[df['fecha_pedido'].dt.quarter == 4]\n",
        "    sns.scatterplot(data=q4_data, x='fecha_pedido', y='ventas', color=ROJO1, s=100, ax=ax, zorder=5)\n",
        "\n",
        "    ax.set_title('Total de Ventas Trimestrales en Buenos Aires (2016-2019)', loc='left', fontsize=18, color=GRIS1)\n",
        "    ax.set_xlabel('')\n",
        "    ax.set_ylabel('Ventas (miles USD)', fontsize=12, color=GRIS2)\n",
        "    ax.tick_params(axis='x', labelsize=10, labelcolor=GRIS2, rotation=45)\n",
        "    ax.tick_params(axis='y', labelsize=10, labelcolor=GRIS2)\n",
        "\n",
        "    # Annotate Q4 points\n",
        "    for idx, row in q4_data.iterrows():\n",
        "        ax.text(row['fecha_pedido'], row['ventas'] + 10, f\"${row['ventas']:.0f}k\",\n",
        "                color=ROJO1, ha='center', va='bottom', fontsize=10, fontweight='bold')\n",
        "\n",
        "    # Despine for cleaner look\n",
        "    sns.despine(left=True, bottom=True)\n",
        "    ax.grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "    plt.tight_layout()\n",
        "    return ax\n",
        "\n",
        "ax = grafico_linea_ventas_trimestrales(df_bsas)\n",
        "fig = ax.get_figure()\n",
        "plt.show()"
      ],
      "metadata": {
        "id": "cbKlnTmjBL1e",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 441
        },
        "outputId": "a78cc68f-ae59-44fa-86ee-03b107f8ad92"
      },
      "execution_count": 39,
