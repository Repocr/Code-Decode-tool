    {
      "cell_type": "code",
      "source": [
        "df_bsas = ventas.copy()\n",
        "df_bsas['provincia'] = df_bsas['provincia'].str.lower() # Convert to lowercase for case-insensitive query\n",
        "df_bsas = df_bsas.query('provincia == \"buenos aires\"')[['fecha_pedido','ventas']] # Select only fecha_pedido and ventas\n",
        "df_bsas.set_index('fecha_pedido',inplace=True)\n",
        "df_bsas = df_bsas.resample('q').agg({'ventas': 'sum'}) # Aggregate only 'ventas'\n",
        "df_bsas['ventas']= round (df_bsas['ventas']/1000,2)\n",
        "df_bsas= df_bsas.reset_index()\n",
        "df_bsas"
      ],
