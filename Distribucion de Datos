        "df_region = ventas.copy()\n",
        "df_region = df_region[['region','fecha_pedido','ventas']]\n",
        "df_region = pd.crosstab(index=df_region.fecha_pedido,columns=df_region.region,\n",
        "                        values=df_region.ventas,aggfunc='sum')\n",
        "df_region = df_region[['Buenos Aires','Litoral','Norte','Central','Cuyo','Patagónica']]\n",
