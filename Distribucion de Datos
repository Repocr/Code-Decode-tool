    {
      "cell_type": "code",
      "source": [
        "# Importando las bibliotecas\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import matplotlib.dates as mdates\n",
        "\n",
        "# Importando el informe de ventas y actualizando la columna de pedido a tipo fecha\n",
        "ventas = pd.read_csv(\"https://gist.githubusercontent.com/ahcamachod/d2efadda3f3833d65da750c64705ac42/raw/80aef6f3bad210d34b67ed85565f039a30e2d9e0/ventas.csv\",sep=\"#\")\n",
        "ventas[\"fecha_pedido\"] = pd.to_datetime(ventas[\"fecha_pedido\"], format=\"%Y-%m-%d\")\n",
        "\n",
        "# Creando un df con los datos deseados\n",
        "df_am = ventas.copy()\n",
        "bienio = [2018, 2019]\n",
        "df_am = df_am.query('provincia == \"Córdoba\" and fecha_pedido.dt.year == @bienio')[[\"fecha_pedido\", \"ventas\"]]\n",
        "\n",
        "# Agrupando las ventas por inicio del mes (MS)\n",
        "df_am.set_index(\"fecha_pedido\", inplace = True)\n",
        "df_am = df_am.resample(\"MS\").agg(\"sum\")\n",
        "df_am = df_am.reset_index()\n",
        "df_am.head()"
      ],
