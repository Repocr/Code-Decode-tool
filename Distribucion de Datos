        "ax = grafico_linea_ventas_trimestre_bsas()\n",
        "# Removed ax.xaxis.set_majorlocator(mdates.MonthLocator(interval=3)) as it caused an AttributeError\n",
        "# Removed ax.xaxis.set_major_formatter(mdates.DateFormatter('%b')) as it caused an AttributeError\n",
        "x_labels = ['Mar','Jun','Sep','Dic']*4\n",
        "ax.set_xticks(df_bsas.fecha_pedido)\n",
        "ax.set_xticklabels(x_labels,ha='center')\n",
        "\n",
        "texto=[16 *'' +str(i)+ 16*'' for i in range (2016,2020)]\n",
        "ax.text(ax.get_xbound()[0]+35,-80,f'|{texto[0]}]|{texto[1]}]|{texto[2]}]|{texto[3]}]|',fontsize=14,color=GRIS2)\n",
        "\n",
        "ano = 0\n",
        "for i in range (3, len(df_bsas['ventas']),4):\n",
        "  ax.text(x= ax.get_xticks()[i],y=df_bsas['ventas'][i]+20,\n",
        "          s=f'${df_bsas['ventas'][i]}mil',color=AZUL2,\n",
        "          weight='bold',fontsize=10)\n",
        "  ax.text(x= ax.get_xticks()[i]+ 160,y=df_bsas['ventas'][i]+20,\n",
        "          s=f'4 Tri{2016 +ano}',color=GRIS2,\n",
        "          weight='bold',fontsize=10)\n",
        "  ano +=1\n",
        "  fig = ax.get_figure()\n",
        "  plt.show()"
