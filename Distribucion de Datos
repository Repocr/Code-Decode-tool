        "ventas['fecha_pedido'] = pd.to_datetime(ventas['fecha_pedido'],format='%Y-%m-%d')\n",
        "ventas['fecha_envio'] = pd.to_datetime(ventas['fecha_envio'],format='%Y-%m-%d')\n",
