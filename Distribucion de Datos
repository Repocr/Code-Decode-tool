    {
      "cell_type": "code",
      "source": [
        "def grafico_linea_ventas_trimestre_bsas():\n",
        "    fig, ax = plt.subplots(figsize=(14,4))\n",
        "\n",
        "    # The original logic for puntos_a_marcar was flawed; using quarter 4 for highlighting\n",
        "    puntos_a_marcar = [True if q == 4 else False for q in df_bsas['fecha_pedido'].dt.quarter]\n",
        "\n",
        "    ax.plot(df_bsas['fecha_pedido'], df_bsas['ventas'], lw=3, color=AZUL5, marker='o',\n",
        "            markersize=10, markevery=puntos_a_marcar, markerfacecolor=AZUL2)\n",
        "\n",
        "    ax.set_title('Ventas por trimestre en la provincia de Buenos Aires', fontsize=18,\n",
        "                 color=GRIS1, loc='left')\n",
        "    ax.text(0.3, 0.95, 'de 2016 a 2019 (en miles de dólares)', transform=ax.transAxes,\n",
        "            color=GRIS2, fontsize=12, ha='right', va='center')\n",
        "    ax.set_xlabel('')\n",
        "    ax.set_ylabel('')\n",
        "    ax.set_yticklabels([])\n",
        "    ax.set_frame_on(False)\n",
        "    ax.xaxis.set_tick_params(labelsize=14, labelcolor=GRIS2)\n",
        "\n",
        "    ax.tick_params(axis='both', which='both', length=0)\n",
        "    plt.ylim(0,400)\n",
        "\n",
        "    return ax\n",
        "\n",
        "ax = grafico_linea_ventas_trimestre_bsas()\n",
        "fig = ax.get_figure()\n",
        "plt.show()"
      ],
